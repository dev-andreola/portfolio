generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Project {
  id        String   @id @default(uuid())
  title     String
  desc      String
  imageUrl  String
  href      String
  createdAt DateTime @default(now())
}

model Post {
  id        String   @id @default(uuid())
  title     String
  desc      String
  imageUrl  String
  href      String
  content   String
  createdAt DateTime @default(now())
}
